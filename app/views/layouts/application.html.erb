<!DOCTYPE html>
<html>
<head>
  <title>CatFights</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<div id="page">
    <div id="head">
        <h1>head</h1>
    </div>

    <div id="nav">
      <%= link_to "fights", :controller => "fights", :action => "index" %> |
      <%= link_to "top total", :controller => "top_picture_total", :action => "index" %> |
      <%= link_to "top time period", :controller => "top_picture_time_period", :action => "index" %>
    </div>

    <div id="content">
      <%= yield %>
    </div>

    <div id="footer">
        <h1>footer</h1>
    </div>
</div>

</body>
</html>
